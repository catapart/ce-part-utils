{
  "name": "ce-part-utils",
  "version": "0.0.3",
  "description": "Utilities for adding part and exportparts attributes to a custom HTML element, using classes, ids, tags and other element properties.",
  "type": "module",
  "main": "dist/ce-part-utils.js",
  "module": "dist/ce-part-utils.js",
  "types": "dist/ce-part-utils.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:build": "vite build --config vite.config.tests.ts",
    "test": "npm run test:build && web-dev-server --watch --app-index test-runner/index.html",
    "release": "npm run build && npx @changesets/cli publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@magnit-ce/test-runner": "^0.0.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.4.6",
    "glob": "^11.0.3",
    "typescript": "^5.5.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "license": "CC0-1.0",
  "author": "catapart",
  "keywords": [
    "custom",
    "element",
    "html",
    "part",
    "exportparts",
    "attribute",
    "component",
    "web component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/catapart/ce-part-utils"
  }
}
